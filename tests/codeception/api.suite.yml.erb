class_name: ApiGuy
modules:
    enabled: [PhpBrowser, REST, ApiHelper]
    config:
      PhpBrowser:
          url: http://<%= ENV['CMS_HOST'] %>
      REST:
          url: http://<%= ENV['API_HOST'] %>/api/

env:
    apiary:
        modules:
            config:
                REST:
                    url: http://127.0.0.1:3000/

    cms-rest-api:
        modules:
            config:
                REST:
                    url: http://127.0.0.1:11111/api/v1/

